{"title":"C's Keyboard (rev1)","rules":[{"description":"Use shift+caps_lock as caps_lock","manipulators":[{"from":{"key_code":"caps_lock","modifiers":{"mandatory":["shift"],"optional":["caps_lock"]}},"to":[{"key_code":"caps_lock"}],"type":"basic"}]},{"description":"Change caps_lock to control if pressed with other keys, to escape if pressed alone.","manipulators":[{"from":{"key_code":"caps_lock","modifiers":{"optional":["any"]}},"to":[{"key_code":"left_control"}],"to_if_alone":[{"key_code":"escape"}],"type":"basic"}]},{"description":"Use left_control as Hyper","manipulators":[{"from":{"key_code":"left_control"},"to":[{"key_code":"right_shift","modifiers":["right_control","right_alt","right_shift","right_gui"]}],"type":"basic"}]},{"description":"Open Obsidian","manipulators":[{"from":{"key_code":"o","modifiers":{"mandatory":["right_control","right_alt","right_shift","right_gui"]}},"to":[{"shell_command":"open -a 'Obsidian.app'"}],"type":"basic"}]},{"description":"Open VSCode","manipulators":[{"from":{"key_code":"v","modifiers":{"mandatory":["right_control","right_alt","right_shift","right_gui"]}},"to":[{"shell_command":"open -a 'VSCode.app'"}],"type":"basic"}]}]}